[frontends]
frontend1 ansible_host=frontend1.79ab.internal

[apps]
app1 ansible_host=app1.79ab.internal
app2 ansible_host=app2.79ab.internal

[appdbs]
appdb1 ansible_host=appdb1.79ab.internal

[support]
support1 ansible_host=support1.79ab.internal

[threetierapp:children]
frontends
apps
appdbs
support

[threetierapp:vars]
timeout=60
ansible_become=yes
ansible_user=ec2-user
ansible_ssh_private_key_file="~/.ssh/79abkey.pem"
ansible_ssh_common_args="-o StrictHostKeyChecking=no"
